<% provide(:title, @user.name) %>
<div class="row" >
  <aside class="col-md-4" >
    <section class="user_info" >
      <h1>
        <%= gravatar_for @user %><br>
        Name: <%= @user.name %><br>
        Email: <%= @user.email %><br>
        User is admin?: <%= user_is_admin? %><br>
      </h1>
    </section>
  </aside>
</div>
